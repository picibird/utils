  "author": "Marcus Specht <me@masp.io> (https://picibird.com)",
  "exports": {
    "./templates/*": "./.dist/templates/*"
  },
  "main": "./.dist/index.js",
  "module": "./.dist/index.es.js",
  "types": "./.dist/index.d.ts",
  "files": [
    ".dist/**/*",
    ".dist/index.d.ts",
    "!**/*.map"
